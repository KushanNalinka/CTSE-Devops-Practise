{
    "family": "gateway-taskdef",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::<YOUR_ACCOUNT_ID>:role/ecsTaskExecutionRole",
    "containerDefinitions": [
      {
        "name": "gateway",
        "image": "PLACEHOLDER",
        "essential": true,
        "portMappings": [
          { "containerPort": 5000, "protocol": "tcp" }
        ]
      }
    ]
  }
  